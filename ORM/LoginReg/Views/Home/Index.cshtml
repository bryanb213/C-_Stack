@model User

<h1>Add a User!</h1>


<Div>
<form asp-action="Register" asp-controller="Home" method="post">
    <span asp-validation-for="FirstName"></span>
    <label asp-for="FirstName"></label>
    <input type="text" asp-for="FirstName">
    <br/>

    <span asp-validation-for="LastName"></span>
    <label asp-for="LastName"></label>
    <input type="text" asp-for="LastName">
    <br/>

    <span asp-validation-for="Email"></span>
    <label asp-for="Email"></label>
    <input type="email" asp-for="Email">
    <br/>


    <span asp-validation-for="Password"></span>
    <label asp-for="Password"></label>
    <input type="password" asp-for="Password">
    <br/>

    <span asp-validation-for="Confirm"></span>
    <label asp-for="Confirm">Confirm Password</label>
    <input type="password" asp-for="Confirm">
    <br/>


    <input type="submit" value="Submit">
</form>
</Div>

