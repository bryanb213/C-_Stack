@model HogwartsStudent
<!DOCTYPE html>
<html>

<head>
    <meta charset='utf-8'>
    <title>Index</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
</head>

<body>
    <div>
    <h1><u>Submitted Info!</u></h1>
    <table class="table-hover table-bordered">
        <thead>
          <tr>
            <th>Name</th>
            <th>Location</th>
            <th>Language</th>
            <th>Comment</th>
          </tr>
        </thead>
        <tbody>
          <tr class="danger">
            <td>@Model.Name</td>
            <td>@Model.Location</td>
            <td>@Model.Language</td>
            <td>@Model.Comment</td>
            
          </tr>
        </tbody>
      </table>
<button><a href="/">Go Back</a></button>
    </div>
</body>

</html>